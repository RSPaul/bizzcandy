    <% include _layouts/header %>

    <div class="container main-container headerOffset">
    
        <!-- Main component call to action -->
    
        <div class="row">
    
            <!--left column-->
    
            <div class="col-lg-3 col-md-3 col-sm-12">
                <% include _layouts/sitenav.ejs %>
            </div>
    
            <!--right column-->
            <div class="col-lg-9 col-md-9 col-sm-12">
    
                <div class="row  categoryProduct xsResponse clearfix">                
                    <!--/ .item-->
                     
                    <% if (count > 0) { %>
                        <% brands.forEach(function(b){ %>
                        <% if (loggedIn) { %>
                        <div class="item h col-sm-4 col-lg-4 col-md-4 col-xs-6">
                        <% } else {%>
                        <div class="item col-sm-4 col-lg-4 col-md-4 col-xs-6">
                        <% } %>
                            <div class="product">                        
                                <div class="image">                            
                                    <a href="/products/warehouse_brand/<%= b.slug %>/<%= warehouse %>" target="_blank">
                                        <% if(b.image == "" || !b.image) { %>
                                            <img id="noimage" src="/images/noimage.jpeg">
                                        <% } else { %>
                                            <img src="<%= brandImageUrl %>/<%= b.image %>" alt="" class="img-responsive">
                                        <% } %>
                                    </a>                           
                                </div>
                                <div class="description">
                                    <h4><a href="/products/warehouse_brand/<%= b.slug %>/<%= warehouse %>"><%= b.name %></a></h4>                   
                                </div>
                            </div>
                        </div>
                        <% }); %>
                    <% } else {%>
                    <p>There are no brands available for the selected warehouse.</p>
                    <% }%>
    
                    
                    <!--/.item-->                                             
                </div>                      
            </div>
            <!--/right column end-->
        </div>
        <!-- /.row  -->
    </div>
    <!-- /main container -->
    
    <% include _layouts/footer %>